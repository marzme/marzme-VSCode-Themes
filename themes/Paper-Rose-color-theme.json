{
    "name": "Paper Rose",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#997665"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#CAC1AB"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#CAC1AB"
            }
        },
        {
            "name": "Invalid - Deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#736923"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#736923"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.other",
                "keyword.control",
                "punctuation.section.group",
                "punctuation.section.braces"
            ],
            "settings": {
                "foreground": "#694623"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#8A5565"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#694623"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#694623"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name",
                "variable.parameter",
                "keyword.operator",
                "meta"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#7F5C5C"

            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#994650"
            }
        },
        {
            "name": "Functions2",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#695C5C"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.tag",
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#694623"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#734623"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant"
            ],
            "settings": {
                "foreground": "#994650"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#694623"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#975744"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#975744"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#975744"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#975744"
            }
        }
    ],
    "colors": {
        "titleBar.activeBackground": "#CAC1AB",
        "titleBar.activeForeground": "#6B4848",
"tab.activeBackground": "#B6AD97",
"tab.activeForeground": "#937070",
"tab.inactiveBackground": "#DED5BF",
"tab.inactiveForeground": "#6B4848",
"editor.background": "#CAC1AB",
        "editor.foreground": "#7F5C5C",
"activityBar.background": "#DED5BF",
        "activityBar.foreground": "#7F5C5C",
"sideBar.background": "#B6AD97",
"statusBar.background": "#DED5BF"
    }
}

{
    "name": "Plastic Code Wrap",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#1E9AE0"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#0B161D"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#0B161D"
            }
        },
        {
            "name": "Invalid - Deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#FF8C00"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#FF8C00"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.other",
                "keyword.control",
                "punctuation.section.group",
                "punctuation.section.braces"
            ],
            "settings": {
                "foreground": "#FFAA00"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#FFAA00"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#79ABFF"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#79ABFF"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name",
                "variable.parameter",
                "keyword.operator",
                "meta"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#F8F8F8"

            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#FF7FFF"
            }
        },
        {
            "name": "Functions2",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#F8F8F8"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.tag",
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#FFAA00"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant"
            ],
            "settings": {
                "foreground": "#FF7FFF"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FFAA00"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#55E439"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#55E439"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#55E439"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#55E439"
            }
        }
    ],
    "colors": {
        "titleBar.activeBackground": "#0B161D",
        "titleBar.activeForeground": "#E4E4E4",
"tab.activeBackground": "#000209",
"tab.activeForeground": "#FFFFFF",
"tab.inactiveBackground": "#1F2A31",
"tab.inactiveForeground": "#E4E4E4",
"editor.background": "#0B161D",
        "editor.foreground": "#F8F8F8",
"activityBar.background": "#1F2A31",
        "activityBar.foreground": "#F8F8F8",
"sideBar.background": "#000209",
"statusBar.background": "#1F2A31"
    }
}

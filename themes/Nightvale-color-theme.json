{
    "name": "Nightvale",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#5A3536"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#130022"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#130022"
            }
        },
        {
            "name": "Invalid - Deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#9B5014"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#9B5014"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.other",
                "keyword.control",
                "punctuation.section.group",
                "punctuation.section.braces"
            ],
            "settings": {
                "foreground": "#A91231"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#AD5B1F"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#9BC3C3"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#9BC3C3"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name",
                "variable.parameter",
                "keyword.operator",
                "meta"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#9BC3C3"

            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#9A89C3"
            }
        },
        {
            "name": "Functions2",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#529DC7"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.tag",
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#A91231"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#9B0014"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant"
            ],
            "settings": {
                "foreground": "#9A89C3"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#A91231"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#AA9F4C"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#AA9F4C"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#AA9F4C"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#AA9F4C"
            }
        }
    ],
    "colors": {
        "titleBar.activeBackground": "#130022",
        "titleBar.activeForeground": "#87AFAF",
"tab.activeBackground": "#00000E",
"tab.activeForeground": "#AFD7D7",
"tab.inactiveBackground": "#271436",
"tab.inactiveForeground": "#87AFAF",
"editor.background": "#130022",
        "editor.foreground": "#9BC3C3",
"activityBar.background": "#271436",
        "activityBar.foreground": "#9BC3C3",
"sideBar.background": "#00000E",
"statusBar.background": "#271436"
    }
}

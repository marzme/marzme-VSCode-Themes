{
    "name": "Python Idle Tango",
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#73D216"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#2E3436"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#2E3436"
            }
        },
        {
            "name": "Invalid - Deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#FF8C00"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#FF8C00"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.other",
                "keyword.control",
                "punctuation.section.group",
                "punctuation.section.braces"
            ],
            "settings": {
                "foreground": "#8CC4FF"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#8CC4FF"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#D4C4A9"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#D4C4A9"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute-name",
                "variable.parameter",
                "keyword.operator",
                "meta"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#F3F6EE"

            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#F08080"
            }
        },
        {
            "name": "Functions2",
            "scope": [
                "support.function"
            ],
            "settings": {
                "foreground": "#FFE721"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.tag",
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#8CC4FF"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant"
            ],
            "settings": {
                "foreground": "#F08080"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#8CC4FF"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#FFBF00"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#FFBF00"
            }
        }
    ],
    "colors": {
        "titleBar.activeBackground": "#2E3436",
        "titleBar.activeForeground": "#DFE2DA",
"tab.activeBackground": "#1A2022",
"tab.activeForeground": "#FFFFFF",
"tab.inactiveBackground": "#42484A",
"tab.inactiveForeground": "#DFE2DA",
"editor.background": "#2E3436",
        "editor.foreground": "#F3F6EE",
"activityBar.background": "#42484A",
        "activityBar.foreground": "#F3F6EE",
"sideBar.background": "#1A2022",
"statusBar.background": "#42484A"
    }
}
